# Google OAuth Configuration
GOOGLE_CLIENT_ID_IOS=your_ios_client_id_here
GOOGLE_CLIENT_ID_ANDROID=your_android_client_id_here
GOOGLE_CUSTOM_SCHEME_IOS=your_ios_custom_scheme_here
GOOGLE_CUSTOM_SCHEME_ANDROID=your_android_custom_scheme_here

# Backend API Configuration
BACKEND_API_BASE_URL=https://your-api-gateway-url.amazonaws.com/Prod

# AWS S3 Configuration for Audio Uploads
AWS_S3_REGION=eu-north-1
AWS_S3_BUCKET=your-s3-bucket-name
AWS_ACCESS_KEY=your-aws-access-key
AWS_SECRET_KEY=your-aws-secret-key

# Required only if using temporary AWS credentials (access key starts with ASIA)
AWS_SESSION_TOKEN=your-aws-session-token